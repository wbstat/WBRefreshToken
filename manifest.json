{
	"manifest_version": 3,
	"name": "WB Refresh Token",
	"version": "1.2",
	"author": "nikitastore@gmail.com",
	"description": "Расширение позволяет получить refresh_token из личного кабинета Wildberries.",
	"permissions": [
		"cookies",
		"tabs",
		"activeTab"
	],
	"action": {
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png",
			"64": "icons/64.png",
			"128": "icons/128.png"
		},
		"default_title": "WB Refresh Token",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"64": "icons/64.png",
		"128": "icons/128.png"
	},
	"host_permissions": [
		"https://seller.wildberries.ru/*",
		"https://seller-auth.wildberries.ru/*"
	]
}